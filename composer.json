{
    "name": "fivesqrd/atlas-laravel",
    "description": "A Laravel 5 service provider for including the Atlas library.",
    "keywords": ["laravel", "laravel 5"],
    "type":"library",
    "license":"MIT",
    "authors":[
        {
            "name":"Five Squared",
            "homepage":"http://fivesqrd.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "fivesqrd/atlas": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0|~5.0"
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": { "Atlas\\Laravel\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Atlas\\Laravel\\Test\\": "tests/" }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Atlas\\Laravel\\ServiceProvider"
            ],
            "aliases": {
                "Atlas": "Atlas\\Laravel\\Facade"
            }
        }
    }
}